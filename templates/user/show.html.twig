{% extends 'base.html.twig' %}

{% block title %}User
{% endblock %}

{% block body %}
	<div class="content">
		<div class="block block-rounded">
			<div class="block-content block-content-full overflow-x-auto">
				<table class="table table-bordered table-striped">
					<tbody>
						<tr>
							<th>Id</th>
							<td>{{ user.id }}</td>
						</tr>
						<tr>
							<th>Uuid</th>
							<td>{{ user.uuid }}</td>
						</tr>
					
						
						<tr>
							<th>Nom</th>
							<td>{{ user.nom }}</td>
						</tr>
						<tr>
							<th>Prenom</th>
							<td>{{ user.prenom }}</td>
						</tr>
						<tr>
							<th>Mail</th>
							<td>{{ user.mail }}</td>
						</tr>
						<tr>
							<th>Poste</th>
							<td>{{ user.poste }}</td>
						</tr>
						<tr>
							<th>Telephone</th>
							<td>{{ user.telephone }}</td>
						</tr>
					</tbody>
				</table>

				<a href="{{ path('app_user_index') }}">Retour Ã  la liste</a>

			</div>
		</div>
	</div>
{% endblock %}
